#include <bits/stdc++.h>
using namespace std;

void count(string str1, string str2)
{
	int cnt = 0, j = 0;

	for (int i = 0; i < str1.length(); i++) {

		if (str2.find(str1[i]) >= 0
			and j == str1.find(str1[i]))
			cnt++;
   j++;
	}
